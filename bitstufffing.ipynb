   "source": [
    "bs = input(\"Enter Bit Sequence\")\n",
    "def bit_stuffing(bs):\n",
    "    seplist = bs.split(\"11111\")\n",
    "    stuffed = \"111110\".join(seplist)\n",
    "    return stuffed\n",
    "print(bit_stuffing(bs))"
   ]
  }
